    "build": "vite build",
    "serve": "vite preview",
    "test": "ava",
    "noise": "node noise.js",
    "benchmark": "node benchmark.js"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "benchmark": "^2.1.4",
    "esm-seedrandom": "^3.0.5",
    "pngjs": "^6.0.0",
    "vite": "^2.5.1"
  },
  "ava": {
    "timeout": "100s"
  }
